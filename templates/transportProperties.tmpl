FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      transportProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

phase1
{
	transportModel	Newtonian;
	nu nu [0 2 -1 0 0 0 0] 2.87e-6;
	rho rho [ 1 -3  0 0 0 0 0 ] 1000;
}

phase2
{
	transportModel Newtonian;
	nu nu [0 2 -1 0 0 0 0] 1.e-5;
	rho rho [ 1 -3  0 0 0 0 0 ] 1;
}

mug mug [1 -1 -1 0 0 0 0] 0;
mul mul [1 -1 -1 0 0 0 0] ${mul};

rhog rhog [ 1 -3  0 0 0 0 0 ] 1;
rhol rhol [ 1 -3  0 0 0 0 0 ] ${rhol};

sigma sigma [ 1 0 -2 0 0 0 0 ] ${sigma};



