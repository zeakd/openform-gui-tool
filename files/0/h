FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      h;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions          [0 1 0 0 0 0 0];

internalField       uniform 1.e-6;

boundaryField
{
 
    outlet
    {
        type zeroGradient;
    }
    
    bottom
    {
		type 	empty;
	}
	
	top
	{
		type 	empty;
	}

}

